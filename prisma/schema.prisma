generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["jsonProtocol"]
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  directUrl         = env("DIRECT_DATABASE_URL")
}

model projects {
  id   String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name String
}
